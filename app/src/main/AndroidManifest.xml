<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.present_recommendation">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Present_Recommendation">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AfterActivity"></activity>
        <activity android:name=".AfterAfterActivity"></activity>
        <activity android:name=".LoginMainActivity"></activity>


        <activity android:name=".DeliveryActivity"></activity>
        <activity android:name=".DeliveryState"></activity>
        <activity android:name=".GiftActivity"></activity>
        <activity android:name=".RankingActivity"></activity>





    </application>

</manifest>